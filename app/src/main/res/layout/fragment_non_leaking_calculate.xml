<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nonLeakingCalculate">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:background="@drawable/background">

        <androidx.cardview.widget.CardView
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="0dp"
            android:background="@color/colorWhite"
            android:layout_margin="0dp">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Raw Material Calculations : Non-Leaking"
                    android:fontFamily="@font/open_sans"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:padding="18dp"/>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.cardview.widget.CardView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="5dp"
                    android:background="@color/colorWhite"
                    android:layout_margin="13dp">
                    <LinearLayout
                        android:background="@color/colorAccent"
                        android:layout_width="25dp"
                        android:layout_height="match_parent"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Give Inputs"
                            android:textAlignment="center"
                            android:fontFamily="@font/open_sans"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:padding="18dp"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="13dp"
                    android:layout_marginLeft="13dp"
                    app:cardBackgroundColor="@color/colorWhite">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="10dp">

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Pipe Diameter (in mm)"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:maxLines="1"
                                android:textSize="14sp"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Pipe Original Wall Thickness (in mm)"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:maxLines="1"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Pipe Material Strength (in psi) *"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:maxLines="1"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Pipe Working Pressure (in bars) *"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_12"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Minimum Wall Thickness (in mm)"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:maxLines="1"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Total Longitudinal extent of damage (in mm)"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:maxLines="1"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Width of CSM Fiber (in mm) "
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:cursorVisible="true"
                                android:maxLines="1"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_8"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Width of WRM Fiber (in mm) "
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:cursorVisible="true"
                                android:maxLines="1"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="GSM of CSM Fiber (in g/m2)"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            android:maxLines="1"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="GSM of WRM Fiber (in g/m2) "
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme_1">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:maxLines="1"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Select Wrapping Method "
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                            android:theme="@style/Text_box_theme_1">

                            <AutoCompleteTextView
                                android:id="@+id/select_wrapping_NL_11"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="none"
                                android:textColor="@color/color5"
                                android:textSize="14sp" />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Select Type of Wall loss "
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                            android:theme="@style/Text_box_theme_1">

                            <AutoCompleteTextView
                                android:id="@+id/select_wall_Loss_NL_14"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="none"
                                android:textColor="@color/color5"
                                android:textSize="14sp" />

                        </com.google.android.material.textfield.TextInputLayout>
                        <LinearLayout
                            android:id="@+id/first_NL"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/NL_50"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Average Wall Loss Extent (in degrees)"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"
                                app:boxStrokeColor="@color/colorAccent"
                                app:hintTextColor="@color/colorAccent"
                                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                                android:theme="@style/Text_box_theme_1">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:inputType="number"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/color5"
                                    android:textSize="14sp"
                                    android:maxLines="1"
                                    android:cursorVisible="true"
                                    android:textCursorDrawable="@color/colorAccent"/>

                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/NL_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Average Interface Adhesive Thickness (in mm) *"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            app:boxStrokeColor="@color/colorAccent"
                            app:hintTextColor="@color/colorAccent"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:theme="@style/Text_box_theme">

                            <com.google.android.material.textfield.TextInputEditText
                                android:inputType="number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/color5"
                                android:textSize="14sp"
                                android:maxLines="1"
                                android:cursorVisible="true"
                                android:textCursorDrawable="@color/colorAccent"/>

                        </com.google.android.material.textfield.TextInputLayout>
                        <LinearLayout
                            android:id="@+id/third_NL"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/NL_21"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Circumferential defect width (in mm) "
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"
                                app:boxStrokeColor="@color/colorAccent"
                                app:hintTextColor="@color/colorAccent"
                                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                                android:theme="@style/Text_box_theme_1">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:inputType="number"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/color5"
                                    android:maxLines="1"
                                    android:textSize="14sp"
                                    android:cursorVisible="true"
                                    android:textCursorDrawable="@color/colorAccent"/>

                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>

                        <Button
                            android:id="@+id/calculateButton_NL"
                            android:layout_width="match_parent"
                            android:layout_marginTop="20dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="20dp"
                            android:fontFamily="@font/open_sans"
                            android:layout_height="wrap_content"
                            android:text="Calculate"
                            android:padding="20dp"
                            android:textColor="@color/colorWhite"
                            app:backgroundTint="@color/colorAccent"
                            app:elevation="2dp"
                            app:rippleColor="#900"
                            />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="vertical">
                </LinearLayout>
                <androidx.cardview.widget.CardView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="5dp"
                    android:background="@color/colorWhite"
                    android:layout_margin="13dp">

                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/output_0_NL"
                        android:background="@color/colorAccent"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Outputs"
                            android:textColor="@color/colorWhite"
                            android:textAlignment="center"
                            android:fontFamily="@font/open_sans"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:padding="18dp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:background="@color/colorWhite"
                        android:layout_width="25dp"
                        android:layout_height="match_parent"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="13dp"
                    android:layout_marginLeft="13dp"
                    app:cardBackgroundColor="@color/colorAccent">
                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/output_1_NL"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="10dp">

                        <TextView
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="4dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="1dp"
                            android:padding="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/colorWhite"
                            android:text="Weight of Resin (in kg) " />
                        <TextView
                            android:id="@+id/NL_01"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="4dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginBottom="1dp"
                            android:padding="19dp"
                            android:textSize="14sp"
                            android:background="@drawable/rouded_corner"
                            android:textColor="@color/color5"
                            android:text="hello" />

                        <TextView
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="4dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="1dp"
                            android:padding="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/colorWhite"
                            android:text="Weight of Hardener (in kg)" />
                        <TextView
                            android:id="@+id/NL_02"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="4dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginBottom="1dp"
                            android:padding="19dp"
                            android:textSize="14sp"
                            android:background="@drawable/rouded_corner"
                            android:textColor="@color/color5"
                            android:text="hello" />
                        <TextView
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="4dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="1dp"
                            android:padding="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/colorWhite"
                            android:text="CSM Fiber Length (in m)" />
                        <TextView
                            android:id="@+id/NL_03"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="4dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginBottom="1dp"
                            android:padding="19dp"
                            android:textSize="14sp"
                            android:background="@drawable/rouded_corner"
                            android:textColor="@color/color5"
                            android:text="hello" />
                        <TextView
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="4dp"
